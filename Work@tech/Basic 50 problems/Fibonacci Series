		Scanner input  = new Scanner(System.in);
		
		int n = input.nextInt();
		
		//how many fibnocci numbers have to print
		for(int i=0;i<n;i++){
			int number =input.nextInt();
			int a =0;
		    int b = 1;
			
			
			for (int j=0;j<number;j++){	
				
				System.out.print(a+" ");
				
				int c= a+b ;
				a=b;
				b=c;
				
			}
			System.out.println();
			
			
		}
	}
}
